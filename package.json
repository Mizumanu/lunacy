{
  "name": "my-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint \"js/**/*.js\"",
    "format": "prettier --write \"js/**/*.js\" \"style/**/*.css\" \"*.html\"",
    "lint:css": "stylelint \"style/**/*.css\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "fix-imports": "node scripts/fix-imports-to-engine.mjs",
    "tree": "tree assets js scripts style index.html package.json package-lock.json postcss.config.js jest.config.js eslint.config.js .eslintrc.js .prettierrc.js vite.config.js -L 4"
  },
  "dependencies": {
    "vite": "^5.4.8"
  },
  "devDependencies": {
    "@babel/core": "^7.27.3",
    "@babel/preset-env": "^7.27.2",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0-alpha.6",
    "stylelint": "^16.21.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-prettier": "^5.0.3"
  }
